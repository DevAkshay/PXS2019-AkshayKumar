<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android"
>
    <TextView
            android:id="@+id/attrib_name"
            android:textStyle="normal"
            android:height="30dp"
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:gravity="center"
            android:textSize="15sp" android:fontFamily="@font/roboto"/>
    <TextView
            android:id="@+id/attrib_value"
            android:gravity="center"
            android:height="30dp"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:textStyle="normal"
            android:textSize="15sp" android:fontFamily="@font/roboto"/>
</TableRow>